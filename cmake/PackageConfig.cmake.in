# - Config file for the @PACKAGE_UPPER_NAME@ package
# It defines the following variables
#  @PACKAGE_UPPER_NAME@_VERSION      - @PACKAGE_NAME@ version


get_filename_component(_ROOT "${CMAKE_CURRENT_LIST_DIR}" PATH)
# These are IMPORTED targets created by @PACKAGE_UPPER_NAME@Targets.cmake
set(@PACKAGE_UPPER_NAME@_LIBRARIES @PACKAGE_NAME@)
set(@PACKAGE_UPPER_NAME@_VERSION "@VERSION@")
set(@PACKAGE_UPPER_NAME@_FOUND TRUE)
set(@PACKAGE_UPPER_NAME@_BINARY_PATH "${_ROOT}/bin/@PACKAGE_NAME@${CMAKE_EXECUTABLE_SUFFIX}" CACHE INTERNAL "sip binary")
set(@PACKAGE_UPPER_NAME@_DEFAULT_SIP_DIR "${_ROOT}/share/@PACKAGE_NAME@" CACHE INTERNAL "sip dir path")
set(@PACKAGE_UPPER_NAME@_INCLUDE_DIRS "${_ROOT}/include")
set(@PACKAGE_UPPER_NAME@_PYTHON_DIR "${_ROOT}/@INSTALL_PYTHON_DIR@" CACHE INTERNAL "python module path")

if(NOT TARGET @PACKAGE_NAME@)
    add_executable(@PACKAGE_NAME@ IMPORTED)
    set_property(TARGET @PACKAGE_NAME@ PROPERTY IMPORTED_LOCATION ${@PACKAGE_UPPER_NAME@_BINARY_PATH})
endif()
